    "\n",
    "w = np.random.randn(2, 2)\n",
    "graph = nir.NIRGraph.from_list(nir.Linear(weight=w), nir.Linear(weight=w))\n",
    "viz = nirviz.visualize(graph)\n",
    "viz"
   ]
